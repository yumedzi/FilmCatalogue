{
  "parser": "babel-eslint",
  "rules": {
    "react/react-in-jsx-scope": "1",
    "indent": ["error", 2],
    "no-unused-vars": 0,
    "jsx-a11y/href-no-hash": ["error", { "components": ["a"] }] 
  },
  "env": {
       "es6": true,
       "node": true
    },
  "parserOptions": {
    "ecmaVersion": 6,
    "ecmaFeatures": {
       "jsx": true
    }
  }
}